{
  "name": "kb-generator-ts",
  "version": "0.1.8",
  "repository": "git+https://github.com/artrayme/kb-generator-ts.git",
  "author": "artrayme",
  "packageManager": "npm@8.19.2",
  "license": "MIT",
  "type": "commonjs",
  "files": [
    "build"
  ],
  "types": "build/lib/src",
  "main": "build/kb-generator.js",
  "scripts": {
    "build:toc": "markdown-toc README.md -i --maxdepth 4 && prettier --write README.md",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "dev": "npm run clean && tsc --build --watch tsconfig.cjs.json",
    "build:cjs": "tsc --project tsconfig.cjs.json && echo '{\"type\":\"commonjs\"}' > build/package.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "clean": "tsc --build --clean && rm -rf build",
    "test": "vitest",
    "prepack": "npm run build",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "check:types": "tsc --noEmit",
    "build": "webpack --progress --color --config webpack/webpack.prod.js"
  },
  "devDependencies": {
    "@prisma-labs/prettier-config": "0.1.0",
    "@swc/core": "1.3.35",
    "@tsconfig/node16-strictest": "^1.0.4",
    "@tsconfig/recommended": "1.0.2",
    "@types/inquirer": "9.0.3",
    "@types/node": "18.14.0",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-deprecation": "1.3.3",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "execa": "7.0.0",
    "fast-glob": "3.2.12",
    "floggy": "0.3.5",
    "fs-jetpack": "5.1.0",
    "inquirer": "9.1.4",
    "markdown-toc": "1.2.0",
    "prettier": "2.8.4",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "vitest": "0.28.5",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2",
    "webpack-merge": "5.8.0",
    "ts-loader": "9.2.8"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "eta": "^2.0.1",
    "wikibase-sdk": "^8.1.1"
  }
}
